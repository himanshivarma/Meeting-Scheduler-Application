<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:andriod="http://schemas.android.com/apk/distribution"
    xmlns:tools="http://schemas.android.com/tools"
    tools:ignore="ExtraText">

    <uses-permission
        android:name="android.permission.READ_EXTERNAL_STORAGE"
        andriod:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.HOMEPAGE"
        tools:targetApi="31">
        <activity
            android:name=".deletingmeeting"
            android:exported="false"
            android:label="@string/title_activity_deletingmeeting"
            android:theme="@style/Theme.HOMEPAGE" />
        <activity
            android:name=".userlist"
            android:exported="false" />
        <activity
            android:name=".userlogin"
            android:exported="false"
            android:label="@string/title_activity_userlogin"
            android:theme="@style/Theme.HOMEPAGE" />
        <activity
            android:name=".afteralogin"
            android:exported="false"
            android:label="@string/title_activity_afteralogin"
            android:theme="@style/Theme.HOMEPAGE" />
        <activity
            android:name=".meetinginfo"
            android:exported="false"
            android:label="@string/title_activity_meetinginfo"
            android:theme="@style/Theme.HOMEPAGE" />
        <activity
            android:name=".meetingsc"
            android:exported="false"
            android:label="@string/title_activity_meetingsc"
            android:theme="@style/Theme.HOMEPAGE"
            tools:ignore="DuplicateActivity" />
        <activity
            android:name=".ADMINLOGIN"
            android:exported="false"
            android:label="@string/title_activity_adminlogin"
            android:theme="@style/Theme.HOMEPAGE" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>